! UI !TABID_ROOT = -1ui_dialog         `Einstellungen`ui_current_page   gs_ui_current_pageui_page 1, TABID_ROOT, `Einstellungen`, "uiTab_Dimension_hl(1)"gosub "pageGenSet"ui_page 2, TABID_ROOT, `Darstellung Text`, "uiTab_Text_hl(1)"gosub "page2DRep"END  ! -- END -- END -- END -- END -- END -- END -- END -- END -- END -- !! ---------------------------------------------------------------------- !!------------------------------------------------------------------------------------------------! General Settings!------------------------------------------------------------------------------------------------"pageGenSet":    listfieldID = 1    itemID = 1    ui_listfield listfieldID, 0, 0, 444, 265, 1    ui_listitem  itemID, listfieldID, "", 0, "", `Inhalt`           :itemID = itemID + 1    ui_listitem  itemID, listfieldID, "prefix", 1, "", `Prefix`     :itemID = itemID + 1    ui_listitem  itemID, listfieldID, "suffix", 1, "", `Suffix`     :itemID = itemID + 1    ui_listitem  itemID, listfieldID, "", 0, "", `Abstände`                :itemID = itemID + 1    ui_listitem  itemID, listfieldID, "n_begin", 1, "",  `Startzahl`       :itemID = itemID + 1    ui_listitem  itemID, listfieldID, "n_step", 1,  "",   `Schrittweite`   :itemID = itemID + 1    ui_listitem  itemID, listfieldID, "n_end", 1,  "", `Endzahl`           :itemID = itemID + 1    ui_listitem  itemID, listfieldID, "b_divide", 1,  "", `Aufteilen statt Abstand addieren`   :itemID = itemID + 1    ui_listitem  itemID, listfieldID, "distance", 1,  "ui_list_thickness_hl(1)", `Abstand`     :itemID = itemID + 1    ui_listitem  itemID, listfieldID, "n_part", 1,  "", `Anzahl Unterteilungen`                :itemID = itemID + 1return!------------------------------------------------------------------------------------------------! 2D Representation!------------------------------------------------------------------------------------------------"page2DRep":    listfieldID = 2    itemID =