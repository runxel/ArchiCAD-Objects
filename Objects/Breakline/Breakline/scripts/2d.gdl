!Break line ! by Ransom Ratcliff 2015! Graphisoft North America!=======================================================	SET LINE_TYPE gs_line_type	SET FILL gs_fill_type	PEN gs_cont_penIF doubleBreak THEN	GOSUB 200:	!Double Break lineELSE	GOSUB 100:	!Single Break lineENDIF!--- Basic Shared Editable Hotspots ---!	unID=100		!	Length		HOTSPOT2 0, 							0, 				unID, A,			1+256	:unID=unID+1		HOTSPOT2 -0.001, 						0,				unID, A,			3+128	:unID=unID+1		HOTSPOT2 A, 							0,				unID, A,			2		:unID=unID+1		!	Zigzag1 Center		HOTSPOT2 A/2, 							0, 				unID, xCenter1Pos,	1+128	:unID=unID+1		HOTSPOT2 A/2-0.001, 					0,				unID, xCenter1Pos,	3+128	:unID=unID+1		HOTSPOT2 A/2+xCenter1Pos, 				0,				unID, xCenter1Pos,	2		:unID=unID+1		!	Zigzag1 Amplitude Top		HOTSPOT2 A/2+xCenter1Pos-xPeriod/4, 	0, 				unID, yAmplitude,	1+128	:unID=unID+1		HOTSPOT2 A/2+xCenter1Pos-xPeriod/4, 	-0.001,			unID, yAmplitude,	3+128	:unID=unID+1		HOTSPOT2 A/2+xCenter1Pos-xPeriod/4, 	yAmplitude,		unID, yAmplitude,	2		:unID=unID+1		!	Zigzag1 Amplitude Bottom		HOTSPOT2 A/2+xCenter1Pos+xPeriod/4, 	0, 				unID, yAmplitude,	1+128	:unID=unID+1		HOTSPOT2 A/2+xCenter1Pos+xPeriod/4, 	0.001,			unID, yAmplitude,	3+128	:unID=unID+1		HOTSPOT2 A/2+xCenter1Pos+xPeriod/4, 	-yAmplitude,	unID, yAmplitude,	2		:unID=unID+1IF mask THEN	GOSUB 300:	!--- get more editable hotspots ---!ENDIFEND! ------------------------------------------------------100:	!--- Single break line ---	POLY2_B 9, 1+2, gs_fill_pen, gs_back_pen, 			0,							0,				1,			A/2+xCenter1Pos-xPeriod/2,	0,				1,			A/2+xCenter1Pos-xPeriod/4,	yAmplitude,		1,			A/2+xCenter1Pos+xPeriod/4,	-yAmplitude,	1,			A/2+xCenter1Pos+xPeriod/2,	0,				1,			A,							0,				0,			A+xCorner2Pos,				B,				0,			xCorner1Pos,				B,				0,			0,							0,				0	HOTLINE2 0, 0, A, 0RETURN200:	!--- Double break line ---	POLY2_B 13, 1+2, gs_fill_pen, gs_back_pen, 			0,							0,				1,			A/2+xCenter1Pos-xPeriod/2,	0,				1,			A/2+xCenter1Pos-xPeriod/4,	yAmplitude,		1,			A/2+xCenter1Pos+xPeriod/4,	-yAmplitude,	1,			A/2+xCenter1Pos+xPeriod/2,	0,				1,			A,							0,				0,			A+xCorner2Pos,				B,				1,			A/2+xCenter1Pos+xPeriod/2,	B,				1,			A/2+xCenter1Pos+xPeriod/4,	B-yAmplitude,	1,			A/2+xCenter1Pos-xPeriod/4,	B+yAmplitude,	1,			A/2+xCenter1Pos-xPeriod/2,	B,				1,			xCorner1Pos,				B,				0,			0,							0,				0	HOTLINE2 0, 0, A, 0	HOTLINE2 xCorner1Pos, B, A+xCorner2Pos, B		unID=200	!	Zigzag2 Amplitude Top		HOTSPOT2 A/2+xCenter1Pos-xPeriod/4, 	B, 				unID, yAmplitude,	1+128	:unID=unID+1		HOTSPOT2 A/2+xCenter1Pos-xPeriod/4, 	B-0.001,		unID, yAmplitude,	3+128	:unID=unID+1		HOTSPOT2 A/2+xCenter1Pos-xPeriod/4, 	B+yAmplitude,	unID, yAmplitude,	2		:unID=unID+1		!	Zigzag2 Amplitude Bottom		HOTSPOT2 A/2+xCenter1Pos+xPeriod/4, 	B, 				unID, yAmplitude,	1+128	:unID=unID+1		HOTSPOT2 A/2+xCenter1Pos+xPeriod/4, 	B+0.001,		unID, yAmplitude,	3+128	:unID=unID+1		HOTSPOT2 A/2+xCenter1Pos+xPeriod/4, 	B-yAmplitude,	unID, yAmplitude,	2		:unID=unID+1		IF mask=0 THEN GOSUB 300:RETURN300:	!--- Editable Hotspots for Depth and Offset	unID=300	!	Depth		HOTSPOT2 A/2+xCenter1Pos+xCorner2Pos, 		0, 			unID, B, 			1+128		:unID=unID+1		HOTSPOT2 A/2+xCenter1Pos+xCorner2Pos, 		-0.001,		unID, B, 			3+128		:unID=unID+1		HOTSPOT2 A/2+xCenter1Pos+xCorner2Pos, 		B, 			unID, B, 			2			:unID=unID+1	IF parallel THEN		!	Top Left Corner x follows Top Right Corner			HOTSPOT2 0, 					B, 			unID, xCorner2Pos,	1+128		:unID=unID+1			HOTSPOT2 -0.001,				B, 			unID, xCorner2Pos,	3+128		:unID=unID+1			HOTSPOT2 xCorner2Pos,			B, 			unID, xCorner2Pos,	2			:unID=unID+1		!	Parallel Offset			HOTSPOT2 A/2+xCenter1Pos, 					B, 			unID, xCorner2Pos, 			1+128		:unID=unID+1			HOTSPOT2 A/2+xCenter1Pos-0.001, 			B,			unID, xCorner2Pos, 			3+128		:unID=unID+1			HOTSPOT2 A/2+xCenter1Pos+xCorner2Pos, 		B, 			unID, xCorner2Pos, 			2			:unID=unID+1ELSE		!	Top Left Corner x is independent of Top Right Corner			HOTSPOT2 0, 					B, 			unID, xCorner1Pos,	1+128		:unID=unID+1			HOTSPOT2 -0.001,				B, 			unID, xCorner1Pos,	3+128		:unID=unID+1			HOTSPOT2 xCorner1Pos,			B, 			unID, xCorner1Pos,	2			:unID=unID+1	ENDIF	!	Top Right Corner x		HOTSPOT2 A, 					B, 			unID, xCorner2Pos,	1+128		:unID=unID+1		HOTSPOT2 A-0.001,				B, 			unID, xCorner2Pos,	3+128		:unID=unID+1		HOTSPOT2 A+xCorner2Pos,			B, 			unID, xCorner2Pos,	2			:unID=unID+1RETURN