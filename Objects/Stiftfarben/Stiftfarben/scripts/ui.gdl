! some static valuesdict UIUI.mid = 220UI.margin.left   = 15UI.padding.small =  5UI.padding.large = 10UI.comp.width.tiny   =  50UI.comp.width.small  =  90UI.comp.width.stnd   = 120UI.comp.width.large  = 160UI.comp.height.small =  13  ! for text / ui_outfieldUI.comp.height.stnd  =  20  ! for ui_infieldUI.comp.height.large =  25  ! for ui_infield, where you select hatchessUI.comp.height.head  =  UI.comp.height.small+3  ! for headers!..............................................................................ui_current_page  gs_ui_current_pagefile_dependence  "uiTab_2DRepr_hl(1)"file_dependence  "uiTab_Dimension_hl(1)"TABID_ROOT       = -1TABID_GENERAL    = 1TABID_2DREPTEXT  = 2if TABID_GENERAL > 0 then    ui_page  TABID_GENERAL, TABID_ROOT,    `Allgemeine Einstellungen`, "uiTab_Dimension_hl(1)"    gosub "generalPage"endifif TABID_2DREPTEXT > 0 then    ui_page  TABID_2DREPTEXT, TABID_ROOT,  `2D Darstellung und Text`, "uiTab_2DRepr_hl(1)"    gosub "2DRepTextPage"endif! =============================================================================end! ============================================================================="generalPage":    ! === Page 1 ================================================================================        currentY = UI.padding.large    ui_style  0,0        ui_pict 1, 0, currentY    currentY = currentY +65    ui_outfield `Nutzen Sie dieses Objekt zur Platzierung der Stift-Farben und zur Darstellung der Liniennamen und Indexnummern.`, 0, currentY, 180, 65    currentY = currentY +65    ui_outfield `Testen Sie diese Funktion mit Ihrem Drucker/Plotter, um korrekte Ergebnisse zu bekommen`, 0, currentY, 180, 65            ! --- break ---    ui_separator  185, 2,  185, 200    currentY = UI.padding.large    UI.margin.x = 200    UI.margin.rcol.r = 351        ui_outfield   `Stil`,                 UI.margin.x,      currentY,     UI.comp.width.stnd,  UI.comp.height.small    ui_infield{3} "iPenColorSymbStyle",   UI.margin.rcol.r, currentY-4,   UI.comp.width.small, UI.comp.height.stnd     currentY = currentY + UI.comp.height.large        ui_outfield   `Anzahl Farben`,        UI.margin.x,      currentY,     UI.comp.width.stnd,  UI.comp.height.small    ui_infield    "nColors",              UI.margin.rcol.r, currentY-4,   UI.comp.width.small, UI.comp.height.stnd     currentY = currentY + UI.comp.height.large        ui_outfield   `Farben pro Reihe`,     UI.margin.x,      currentY,     UI.comp.width.stnd,  UI.comp.height.small    ui_infield    "nFillsInARow",         UI.margin.rcol.r, currentY-4,   UI.comp.width.small, UI.comp.height.stnd     currentY = currentY + UI.comp.height.large        ui_infield{3} "bFrame",               UI.margin.x,      currentY-4,   241,                 UI.comp.height.stnd,                   7, "", 2, 1,                   0,0,0,0,                   1, `Rahmen`, 0,                   2, `Rahmen`, 1  \                   ui_tooltip  `Erzeugt einen Rahmen um die gesamte Stiftpalette.`    currentY = currentY + UI.comp.height.large        ui_infield{3}  "bLockSymbol",         UI.margin.x,       currentY-9,  241,                 UI.comp.height.stnd,                   7, "", 2, 1,                   0,0,0,0,                   1, `Automatisch orientieren`, 0,                   2, `Automatisch orientieren`, 1  \                   ui_tooltip  `Wenn aktiv, so wird die Plandrehung ignoriert.`return"2DRepTextPage":    ! === Page 2 ================================================================================        currentY = UI.padding.large    ! 2D    ui_style  0,1    UI_OUTFIELD  `2D-Darstellung`,   UI.margin.left, currentY,   UI.comp.width.stnd, UI.comp.height.head    currentY = currentY + UI.padding.large + UI.comp.height.head    ui_style  0,0        ui_outfield  `Schraffur`,    UI.margin.left,    currentY,    UI.comp.width.stnd,  UI.comp.height.small    currentY = currentY + UI.padding.large + UI.comp.height.small -4    ui_infield   "gs_fill_type", UI.margin.left+10, currentY,    UI.comp.width.large, UI.comp.height.large    currentY = currentY + UI.padding.large + UI.comp.height.large        ui_outfield  `Schraffur-Vordergrundstift`,  UI.margin.left, currentY,   UI.comp.width.stnd, UI.comp.height.small    currentY = currentY + UI.padding.large + UI.comp.height.small -4    ui_infield   "gs_fill_pen",  UI.margin.left+10, currentY,               UI.comp.width.tiny, UI.comp.height.stnd    currentY = currentY + UI.padding.large + UI.comp.height.stnd        ui_outfield  `Hintergrundstift`, UI.margin.left,    currentY,       UI.comp.width.stnd,  UI.comp.height.small    currentY = currentY + UI.padding.large + UI.comp.height.small -4    ui_infield   "gs_back_pen",      UI.margin.left+10,   currentY,     UI.comp.width.tiny, UI.comp.height.stnd    currentY = currentY + UI.padding.large + UI.comp.height.stnd            ui_outfield  `Farbschraffurentyp`,   UI.margin.left, currentY,      UI.comp.width.stnd,  UI.comp.height.small    currentY = currentY + UI.padding.large + UI.comp.height.small -4    ui_infield   "fillType",         UI.margin.left+10, currentY,       UI.comp.width.large, UI.comp.height.large        ! --- break ---    ui_separator  UI.mid, 5,  UI.mid, 240    currentY = UI.padding.large    ! Font    ui_style  0,1    ui_outfield `Zeichensatz`, UI.mid+10, currentY,     UI.comp.width.stnd, UI.comp.height.head    currentY = currentY + UI.padding.large + UI.comp.height.head    ui_style  0,0        ui_outfield `Schrifttyp`,  UI.mid+10, currentY,      UI.comp.width.stnd,   UI.comp.height.small    currentY = currentY + UI.padding.large + UI.comp.height.small -4    ui_infield "fontType",     UI.mid+10*2, currentY,    UI.comp.width.stnd+20, UI.comp.height.small    currentY = currentY + UI.padding.large + UI.comp.height.small        ui_outfield `Größe`,       UI.mid+10, currentY,      UI.comp.width.stnd,   UI.comp.height.small    currentY = currentY + UI.padding.large + UI.comp.height.small -4    ui_infield "Font_size",    UI.mid+10*2, currentY,    UI.comp.width.tiny,  UI.comp.height.small    currentY = currentY + UI.padding.large + UI.comp.height.small        ui_outfield `Schrifttyp`,  UI.mid+10, currentY,      UI.comp.width.stnd,   UI.comp.height.small    currentY = currentY + UI.padding.large + UI.comp.height.small -4        ui_infield{3} "BOL", UI.mid+10, currentY, 24, 24,                  6, "",                  2,                  1, 16, 16, 16, 16,                  "ui_bold_hl(1)", "", 0,                  "ui_bold_hl(1)", "", 1        ui_infield{3} "ITA", UI.mid+10+25, currentY, 24, 24,                  6, "",                  2,                  1, 16, 16, 16, 16,                  "ui_italic_hl(1)", "", 0,                  "ui_italic_hl(1)", "", 1        ui_infield{3} "UND", UI.mid+10+25*2, currentY, 24, 24,                   6, "",                   2,                   1, 16, 16, 16, 16,                   "ui_underline_hl(1)", "", 0,                   "ui_underline_hl(1)", "", 1    currentY = currentY + UI.padding.large + 20        ui_outfield `Text-Stift`,  UI.mid+10, currentY,      UI.comp.width.stnd,  UI.comp.height.small    currentY = currentY + UI.padding.large + UI.comp.height.small -4    ui_infield "textPen",      UI.mid+10*2, currentY,    UI.comp.width.tiny, UI.comp.height.stndreturn