!--- Master script ---!dict ltype  !globalconsid = 1	! consecutive id used as index! we will brutforce our way through the list :)for i=1 to max_id	r = request("Name_of_line_type", i, lt_name)		if lt_name # "" then		! line type exists		ltype.name[consid] = lt_name				! get the real index		ltype.real_id[consid] = ind(line_type, lt_name)				consid = consid+1	endifnext i