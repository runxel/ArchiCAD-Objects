!!! EPS thingieeps = 0.0001!!! secure the minimal sizesA = MAX(0.21, A)B = MAX(0.297, B)!!! LochrandIF endformat THEN	n_punch_margin = 0.02ELSE	n_punch_margin = 0ENDIF!!! horizontale UnterteilungenIF (B MOD 0.297) = 0 THEN		hcounter = INT((B/0.297)-1)			! hcounter muss ungerade sein	ELSE		hcounter = INT(B/0.297)ENDIF!!! keine Midmarker wenn Höhe = A4IF (B <= 0.297) THEN	b_mid = 0ENDIF!!! kleiner als A3 oder ohne Lochrand: kein SchrägfalzmarkerIF (B <= 0.297) OR ("endformat" = "3") THEN 	b_diagonal = 0ENDIF!!!  Druckrand = 0 wenn ausIF NOT(b_margin) THEN	n_margin = 0ENDIF