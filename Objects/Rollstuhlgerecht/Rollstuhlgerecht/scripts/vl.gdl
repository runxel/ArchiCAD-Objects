! param scriptlock "A", "B"if pen_wheelie < 1 then	pen_wheelie = 1	parameters pen_wheelie = pen_wheelieendifif pen_area < 1 then	pen_area = 1	parameters pen_area = pen_areaendifif pen_circle < 1 then	pen_circle = 1	parameters pen_circle = pen_circleendifif gs_fill_pen < 1 then	gs_fill_pen = 1	parameters gs_fill_pen = gs_fill_penendifif not(b_showWheelie) then	hideparameter "lt_wheelie",					"pen_wheelie",					"fill_wheelie",					"gs_fill_pen",					"gs_back_pen"endifif not(b_showArea) then	hideparameter "lt_area", "pen_area"endifif not(b_showCircle) then	hideparameter "lt_cicle", "pen_circle"endifif not(b_showArea) and not(b_showCircle) then	lock "b_showWheelie"endifif not(b_showWheelie) and not(b_showCircle) then	lock "b_showArea"endifif not(b_showWheelie) and not(b_showArea) then	lock "b_showCircle"endif